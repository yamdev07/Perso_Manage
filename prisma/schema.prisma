generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employee {
  id             Int       @id @default(autoincrement())
  createdAt      DateTime  @default(now())
  dateDebutGrade DateTime
  dateFinGrade   DateTime?
  dateNaissance  DateTime
  grade          String
  indice         Int
  matricule      String    @unique
  nom            String
  prenoms        String
}
