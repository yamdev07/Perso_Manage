generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employee {
  id           Int      @id @default(autoincrement())
  matricule    String   @unique
  nom          String
  prenoms      String
  dateNaissance DateTime
  grade        String
  dateDebutGrade DateTime
  dateFinGrade   DateTime?
  indice       Int
  createdAt    DateTime @default(now())
}

